<% var head = () => { %>
    <title>Blog Posts</title>
    <!-- Add necessary CSS for styling -->
<% } -%>

<% var body = () => { %>
<div class="container">
    <h1>Blog Posts</h1>

    <ul class="post-list">
        <% posts.forEach(function(post) { %>
            <li>
                <a href="/post/<%= post._id %>"><%= post.title %></a>
            </li>
        <% }); %>
    </ul>
</div>

<!-- Include additional styling or scripts as needed -->
<% } -%>
